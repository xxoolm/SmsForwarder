<?xml version="1.0" encoding="utf-8"?><!--有公司logo-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:opacity="opaque">
    <item android:drawable="@color/xui_config_color_white" />

    <item
        android:bottom="@dimen/xui_config_app_logo_bottom"
        tools:ignore="PrivateResource">
        <bitmap
            android:gravity="center"
            android:src="@drawable/ic_splash_app_logo" />
    </item>

    <item
        android:bottom="@dimen/xui_config_company_logo_bottom"
        tools:ignore="PrivateResource">
        <bitmap
            android:gravity="bottom"
            android:src="@drawable/ic_splash_company_logo" />
    </item>
</layer-list>